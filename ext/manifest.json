{
  "manifest_version": 2,

  "name": "jawspeak-TabCarousel",
  "version": "0.5.6",

  "description": "Auto cycle through your Chrome tabs. Forked version of another app, to add scrolling window support for jira pages.",
  "homepage_url": "https://github.com/jawspeak/TabCarousel",

  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },

  "background": {
    "page": "background.html"
  },

  "options_page": "options.html",

  "browser_action": {
    "default_icon": "images/icon_32.png"
  },

  "permissions": [
    "tabs",
	"activeTab",
    "http://*/",
    "https://*/"
  ]
}
